fn int factorial(int n) {
    if n == 0 {
        1
    } else {
        n * factorial(n - 1)
    }
}

fn int fib(int n) {
    if n == 0 {
        0
    } else if n == 1 {
        1
    } else {
        let int a = fib(n - 1);
        let int b = fib(n - 2);

        a + b
    }
}

fn int main() {
    fib(40) + factorial(12)
}
