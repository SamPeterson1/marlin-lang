fn int factorial(int n) {
    if n == 0 {
        1
    } else {
        n * factorial(n - 1)
    }
}

fn int fib(int n) {
    if n == 0 {
        0
    } else if n == 1 {
        1  
    } else {
        fib(n - 1) + fib(n - 2)
    }
}

fn int add(*int a, int b) {
    *a + b
}

fn int main() {
    let int a = 1;
    add(&a, 3)
}
