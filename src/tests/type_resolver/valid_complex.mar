// Complex valid scenarios

struct Node {
    int data;
    Node* next;
    Node* prev;
    Container* parent;
}

struct Container {
    Node* head;
    Node* tail;
    int size;
}

struct AllTypes {
    int int_val;
    double double_val;
    bool bool_val;
    char char_val;
    int* int_ptr;
    double& double_ref;
    int[] int_array;
    Node* node_ptr;
    Container container;
}
