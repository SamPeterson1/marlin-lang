fn factorial(int n) -> int {
    if n == 0 {
        1
    } else {
        n * factorial(n - 1)
    }
}

fn fib(int n, Foo b) -> int {
    if n == 0 {
        0
    } else if n == 1 {
        1  
    } else {
        fib(n - 1) + fib(n - 2)
    }
}

fn add(*int a, int b) -> int {
    *a + b
}

fn main() -> int {
    let int a = 1;
