struct Test {
    *int a;
    *int b;
    int c;
}

fn modify_test(*Test test, int val) {
    test->c = val;
}

fn int main() {
    let int a = 15;
    let int b = 10;
    
    let Test test = Test {
        a: &a,
        b: &b,
        c: 0,
    };

    a = 3;

    modify_test(&test, 2);

    *test.a + *test.b + test.c
}